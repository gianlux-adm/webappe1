    <meta charset="utf-8">
    <meta http-equiv="X-UA-Compatible" content="IE=edge">
    <title>@ViewBag.Title</title>
    <meta content="width=device-width, initial-scale=1, maximum-scale=1, user-scalable=no" name="viewport">
    <link href="~/Content/AdminLte/bootstrap/css/bootstrap.min.css" rel="stylesheet" />
    <link href="~/Content/AdminLte/css/font-awesome.min.css" rel="stylesheet" />
    <link href="~/Content/AdminLte/css/ionicons.min.css" rel="stylesheet" />
    <link href="~/Content/AdminLte/css/AdminLTE.min.css" rel="stylesheet" />
    <link href="~/Content/AdminLte/css/skins/_all-skins.min.css" rel="stylesheet" />
    <link href="~/Content/Site.css?v=1" rel="stylesheet" />
    <link href="~/Scripts/AdminLte/plugins/datatables/dataTables.bootstrap.css" rel="stylesheet" />
    <link href="~/Content/AdminLte/jQueryUi/jquery-ui.css" rel="stylesheet" />
    <link href="~/Content/bootstrap-switch/bootstrap-switch.css" rel="stylesheet" />
    <link href="~/Content/boostrap-duallistbox/bootstrap-duallistbox.css" rel="stylesheet" />
    <link href="~/Scripts/AdminLte/plugins/timepicker/bootstrap-timepicker.css" rel="stylesheet" />
    <link href="~/Scripts/bootstrap-datetimepicker.css" rel="stylesheet" />
    <link href="~/Scripts/AdminLte/plugins/select2/select2.css" rel="stylesheet" />
    <!-- HTML5 Shim and Respond.js IE8 support of HTML5 elements and media queries -->
    <!-- WARNING: Respond.js doesn't work if you view the page via file:// -->
    <!--[if lt IE 9]>
        <script src="https://oss.maxcdn.com/html5shiv/3.7.3/html5shiv.min.js"></script>
        <script src="https://oss.maxcdn.com/respond/1.4.2/respond.min.js"></script>
    <![endif]-->
</head>
<body class="hold-transition skin-red @(ViewBag.NavBarSide==true ? "sidebar-collapse" : String.Empty)">
    <div class="wrapper">
        <header class="main-header">
            @if (Model.ShowLeftMenu)
            {
                <a href="/Home/Index" class="logo" style="background-color:white">
                    <span class="logo-lg">
                        <img src="~/Content/Images/logo.png" height="40" class="logoImg" />
                    </span>
                </a>
            }
            @{Html.RenderPartial("_TopMenu", Model);}
        </header>
        @if (Model.ShowLeftMenu)
        {
            Html.RenderPartial("_LeftMenu");
        }
        <div class="content-wrapper" style="overflow:auto;@(Model.ShowLeftMenu==false ? "margin-left:0px;" : String.Empty)">
            <section class="content"
                     @if (Model.ShowLeftMenu == false) { <text> style="padding:0px!important;" </text> }>
                @if (IsSectionDefined("breadcrumb"))
                {
                    @RenderSection("breadcrumb")
                }
                @{Html.RenderPartial("ValidationSummary", Model);}
                @RenderBody()
                @{Html.RenderPartial("_ModalConfirm");}
                <div id="modal" class="modal fade in">
                    <div class="modal-dialog modal-lg">
                        <div id="modalContent" class="modal-content" style="border-radius:5px">

                        </div>
                    </div>
                </div>
            </section>
        </div>
        <!--
        <footer class="main-footer">
          <div class="pull-right hidden-xs">
            <b>Version</b> 1.0.0
          </div>
          <strong>Copyright &copy; 2015-2020 <a href="http://www.zaffrani.it/"> F.lli Zaffrani srl</a>.</strong> All rights reserved.
        </footer>-->
    </div>
    <script src="~/Scripts/AdminLte/plugins/jQuery/jQuery-2.1.4.min.js"></script>
    <script src="~/Scripts/jquery.validate.min.js"></script>
    <script src="~/Scripts/jquery.validate.unobtrusive.min.js"></script>
    <script src="~/Scripts/jquery.unobtrusive-ajax.min.js"></script>
    <script src="~/Scripts/AdminLte/plugins/jQueryUI/jquery-ui.min.js"></script>
    <script src="~/Scripts/AdminLte/noconflict.js"></script>
    <script src="~/Scripts/AdminLte/bootstrap/bootstrap.min.js"></script>
    <script src="~/Scripts/AdminLte/app.js?v=1"></script>
    <script src="~/Scripts/AdminLte/plugins/fullcalendar/moment.min.js"></script>
    <script src="~/Scripts/AdminLte/plugins/fullcalendar/fullcalendar.js"></script>
    <script src="~/Scripts/bootstrap.datetimepicker.js"></script>
    <script src="~/Scripts/bootstrap-switch/bootstrap-switch.js"></script>
    <script src="~/Scripts/boostrap-duallistbox/jquery.bootstrap-duallistbox.js"></script>
    <script src="~/Scripts/AdminLte/plugins/timepicker/bootstrap-timepicker.js"></script>
    <script src="~/Scripts/jquery.typeahead.js"></script>
    <script src="~/Scripts/AdminLte/plugins/datatables/jquery.dataTables.js"></script>
    <script src="~/Scripts/AdminLte/plugins/datatables/dataTables.bootstrap.js"></script>
    <script src="~/Scripts/AdminLte/plugins/datatables/extensions/ColumnFilter/jquery.dataTables.columnFilter.js"></script>
    <script src="~/Scripts/AdminLte/plugins/datatables/extensions/Buttons-1.1.0/js/dataTables.buttons.js"></script>
    <script src="~/Scripts/AdminLte/plugins/datatables/extensions/Buttons-1.1.0/js/buttons.bootstrap.js"></script>
    <script src="~/Scripts/AdminLte/plugins/datatables/extensions/Buttons-1.1.0/js/buttons.colVis.js"></script>
    <script src="~/Scripts/custom.js"></script>
    <script src="~/Scripts/bootstrap-fileinput/js/fileinput.js"></script>
    <script src="~/Scripts/AdminLte/plugins/select2/select2.js"></script>
    <script src="~/Scripts/AdminLte/plugins/fullcalendar/momentlocale/@(System.Threading.Thread.CurrentThread.CurrentUICulture.TwoLetterISOLanguageName).js"></script>
    <script src="~/Scripts/AdminLte/plugins/fullcalendar/locale/@(System.Threading.Thread.CurrentThread.CurrentUICulture.TwoLetterISOLanguageName).js"></script>
    @RenderSection("scripts", false)
    <script src="~/Scripts/StopVideoOnModalClose.js" type="text/javascript"></script>

</body>
</html>